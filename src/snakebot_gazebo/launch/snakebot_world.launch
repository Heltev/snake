<launch>	  
	<include file="$(find gazebo_ros)/launch/empty_world.launch">	    
		<arg name="world_name" value="$(find snakebot_gazebo)/worlds/snakebot.world"/>	    
		<arg name="gui" value="true"/>	  
	</include>
	
	<param name="robot_description" command="$(find xacro)/xacro.py '$(find snakebot_description)/urdf/snakebot.xacro'" />

	

	<!-- param '-J <joint_name> <value>' to set the initial angle of the links -->
	<!-- push robot_description to factory and spawn robot in gazebo -->
	<node name="snakebot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"
	 args="-urdf -param robot_description -model snakebot
	 -J joint_01 0.2
	 -J joint_02 0.2
	 -J joint_03 0.2
	 -J joint_04 0.2
	 -J joint_05 0.2
	 -J joint_06 -0.2
	 -J joint_07 -0.2
	 -J joint_08 -0.2
	 -J joint_09 -0.2
	 -J joint_10 -0.2
	 " /> 
	
	
	</launch>
